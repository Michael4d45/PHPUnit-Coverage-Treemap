{
    "name": "michael4d45/coverage-treemap",
    "type": "library",
    "description": "PHPUnit extension that generates an interactive squarified treemap visualization of code coverage",
    "version": "1.0.0",
    "keywords": [
        "phpunit",
        "coverage",
        "treemap",
        "visualization",
        "code-coverage"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "phpunit/phpunit": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "CoverageTreemap\\CoverageTreemap\\": "src/CoverageTreemap/"
        }
    },
    "extra": {
        "resources": {
            "coverage-treemap": "resources/coverage-treemap"
        }
    }
}

